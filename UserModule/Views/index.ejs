<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Users List</title>
</head>

<body>
    <h1>Users List</h1>
    <table>
        <thead>
            <tr>
                <th>SN</th>
                <th>Full Name</th>
                <th>Email</th>
                <th>Gender</th>
                <th>Job Title</th>
                <th>Salary</th>
            </tr>
        </thead>
        <tbody>
            <% allUsers.forEach((user, index)=> { %>
                <tr>
                    <td>
                        <%= index+1 %>
                    </td>
                    <td>
                        <%= user.first_name %> - <%= user.last_name %>
                    </td>
                    <td>
                        <%= user.email %>
                    </td>
                    <td>
                        <%= user.gender %>
                    </td>
                    <td>
                        <%= user.job_title %>
                    </td>
                    <td>
                        <%= user.salary %>
                    </td>
                </tr>
                <% }) %>
        </tbody>
    </table>
</body>

</html>