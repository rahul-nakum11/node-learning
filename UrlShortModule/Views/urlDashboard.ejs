<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>URL List</title>
</head>

<body>
    <h1>URL List</h1>
    <table>
        <thead>
            <tr>
                <th>SN</th>
                <th>Short ID</th>
                <th>Redirect URL</th>
                <th>Visit Count</th>
            </tr>
        </thead>
        <tbody>
            <% urls.forEach((url, index)=> { %>
                <tr>
                    <td>
                        <%= index+1 %>
                    </td>
                    <td>
                        <%= url.shortId %>
                    </td>
                    <td>
                        <%= url.redirectUrl %>
                    </td>
                    <td>
                        <%= url.visitHistory.length %>
                    </td>
                </tr>
                <% }) %>
        </tbody>
    </table>
</body>

</html>